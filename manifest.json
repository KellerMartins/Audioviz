{
  "manifest_version": 3,
  "name": "Soundviz",
  "version": "1.0",
  "description": "Watch your songs like it's 2009!",
  "permissions": ["storage", "sidePanel", "tabCapture"],
  "action": {
    "default_popup": "popup.html"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
